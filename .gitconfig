[include]
  path = ~/.gitconfig_private
[core]
  excludesfile = ~/.gitignore_global
  editor = nvim
[alias]
  a = add
  aa = add --all .
  cm = commit --verbose
  co = checkout
  d = diff
  l = "!. ~/.bin/lib/githelpers && pretty_git_log"
  la = !git l --all
  p = push
  pl = pull
  s = status
[merge]
  tool = vimdiff
[push]
  default = simple
[pull]
  rebase = false
[sendemail]
  annotate = yes
[init]
  defaultBranch = main
