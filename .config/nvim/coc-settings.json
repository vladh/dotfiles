{
  "clangd.semanticHighlighting": true,
  "suggest.autoTrigger": "none",
  "diagnostic.messageTarget": "echo",
  "signature.enable": false,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  }
}
